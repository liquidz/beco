{
 :version "0.0.1"
 :commands [
            ;; official language stack
            {:name "go"
             :image "golang:1.3.3"
             :url "https://registry.hub.docker.com/_/golang/"
             :example "beco go version"}

            {:name "ruby"
             :image "ruby:2.2.0"
             :url "https://registry.hub.docker.com/_/ruby/"
             :example "beco ruby -v"}
            {:name "gem"
             :image "ruby:2.2.0"
             :url "https://registry.hub.docker.com/_/ruby/"
             :example "beco gem -v"}
            {:name "irb"
             :image "ruby:2.2.0"
             :interactive-tty true
             :url "https://registry.hub.docker.com/_/ruby/"
             :example "beco irb -v"}

            {:name "lein"
             :image "clojure:lein-2.5.0"
             :url "https://registry.hub.docker.com/_/clojure/"
             :example "beco lein version\nbeco -i lein repl"}

            {:name "php"
             :image "php:5.6"
             :url "https://registry.hub.docker.com/_/php/"
             :example "FIXME"}

            {:name "node"
             :image "node:0.10"
             :url "https://registry.hub.docker.com/_/node/"
             :example "beco node -v"}
            {:name "npm"
             :image "node:0.10"
             :url "https://registry.hub.docker.com/_/node/"
             :example "beco npm -v"}

            ;; third-party
            {:name "heroku"
             :image "beco/heroku"
             :url "https://registry.hub.docker.com/u/uochan/heroku-toolbelt/"
             :example "#setup\necho \"FROM uochan/heroku-toolbelt\" > Dockerfile\ndocker build --rm -t beco/heroku .\n\nbeco -i -c heroku /heroku_login.sh\nbeco heroku apps"}

            {:name "hubot"
             :image "uochan/hubot"
             :url "https://github.com/liquidz/docker-hubot"
             :example "beco -c hubot npm install\nbeco -i -c hubot bin/hubot"
             :command "redis-server"}

            {:name "bats"
             :image "uochan/bats"
             :url "https://github.com/liquidz/docker-bats"
             :example "beco bats -v"}
            ]
 }
